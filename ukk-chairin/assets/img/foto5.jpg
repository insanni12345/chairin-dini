            item.classList.remove('show');
        });
        if (!!ls) {
            ls.classList.add('show');
            ls.children[0].focus();
            // console.log(ls.children[0].value);
        }
    }

    function valueOthers(e) {
        alasan_text = e.value;
    }
</script>
